const mongoose = require("mongoose")

const AddSchem = mongoose.Schema({
    department_name: {
        type: String,
        require: true
    },
    department_thumbh: {
        type: String,
        require: true
    },
    corse_name: {
        type: String,
        require: true
    },
    corse_thumbh: {
        type: String,
        require: true
    },
    semister: {
        type: String,
        require: true
    },
    subject_name: {
        type: String,
        require: true
    },
    subject_thumbh: {
        type: String,
        require: true
    },
    subject_discription: {
        type: String,
        require: true
    },
    authar: {
        type: String,
        require: true
    },
    book_name: {
        type: String,
        require: true
    },
    book_link: {
        type: String,
        require: true
    },
    book_thumbh: {
        type: String,
        require: true
    },
    book_discription: {
        type: String,
        require: true
    },
    book_rating: {
        type: String,
        require: true
    }
})

module.exports = mongoose.model('Books', AddSchem)